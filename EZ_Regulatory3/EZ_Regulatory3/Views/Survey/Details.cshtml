        @Html.DisplayFor(model => model.DateEnd)
    </div>

    <div class="display-label">Month</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Month)
    </div>

    <div class="display-label">Date Added</div>
    <div class="display-field">
