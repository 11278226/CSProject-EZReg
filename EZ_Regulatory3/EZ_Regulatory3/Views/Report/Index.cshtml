@model IEnumerable<EZ_Regulatory3.Models.Survey>

@{
    ViewBag.Title = "Reporting";
}

<h2>Monthly Reports</h2>
<h3>@ViewBag.Message</h3>
<p>
    @Html.ActionLink("View Current Months Checklists", "Index") |
    @Html.ActionLink("View Unapproved Checklists", "ViewUnapproved") |
    @Html.ActionLink("View All Checklists", "ViewAll")
</p>
<table>
    <tr>
        <th>
            Survey
        </th>
        <th>
            Date Modified
        </th>
        <th>
            Submitted
        </th>
        <th>
            Approved
        </th>
        <th>
            Date Added
        </th>
    </tr>

@foreach (var item in Model) { 
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Title)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DateStart)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DateEnd)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Submitted)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Approved)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DateAdded)
        </td>
    </tr>
}
</table>
